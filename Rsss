<script>
  
  function setCookie(cname,cvalue,exdays) {
        var d = new Date();
        d.setTime(d.getTime() + (exdays*24*60*60*1000));
        var expires = "expires=" + d.toGMTString();
        document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
    }
    function getCookie(name) {
        function escape(s) { return s.replace(/([.*+?\^${}()|\[\]\/\\])/g, '\\$1'); };
        var match = document.cookie.match(RegExp('(?:^|;\\s*)' + escape(name) + '=([^;]*)'));
        return match ? match[1] : 0;
    }
  var pageOverlay = pageOverlay || (function ($) {
    return {
      show: function (message, options) {
        if(!$('#page-overlay').hasClass('active')){
          $('#page-overlay').addClass('active');
          $('#page-overlay .page-loading-image').removeClass('d-none');
        }
      },

      hide: function () {
        if($('#page-overlay').hasClass('active')){
          $('#page-overlay').removeClass('active');
          $('#page-overlay .page-loading-image').addClass('d-none');
        }
      }
    };

  })(jQuery);
               $(":submit").click(function(e) {
              e.preventDefault();
              $("#page-overlay").toggleClass("active");
                var form = $(this).parents('form:first');
               function show_popup(){
                  var isValid = form.isValid();
                  if(isValid){
                    form.submit(); 
                  }else{
                    $("#page-overlay").toggleClass("active");
                  }
               };
               window.setTimeout( show_popup, 3000 );          
             });  
     $(function () {
        showSnow();
        if (!$(".alert").hasClass('no-auto-close')) {
            $(".alert").delay(15000).slideUp(300);
        }
            getBCs();
         
    });

    function funBroadcast(val)
    {
        //Perform Ajax request.
        $.ajax({
            url: "https://indiansmartpanel.com/broadcast" + "/" + val,
            type: 'get',
            success: function(data){
                if(data['recordsFiltered']==1){
                    val = data['data'][0]['id'];
                    swal({
                        title:"<span style='font-size:25px'>" + data['data'][0]['MsgTitle'] + "</span>",
                        html:"<p style='font-size:20px;margin-top:10px'>" + data['data'][0]['MsgText'] + "</p>",
                        type:data['data'][0]['Icon'],
                        confirmButtonText:'<span style="font-size:25px"><i class="fa fa-thumbs-up"></i> Ok! </span>',
                        width: '650px'
                        }).then((result) => {  
                            if(val != 0){
                                setCookie('MsgID',val,30);
                                funBroadcast(val); 
                            }else{  
                                funBroadcast(getCookie("MsgID"));
                            }
                        });
                    if(val == 0){
                            $(".swal2-modal").css('background-color', '#fff');
                    }
                    else{
                            $(".swal2-modal").css('background-color', '#fff');
                    }
                }    
            },
            error: function (xhr, ajaxOptions, thrownError) {
                        var errorMsg = 'Ajax request failed: ' + xhr.responseText;
                
              }
        });
    }

    function showPageNotifications(page,rand)
    {
        //Perform Ajax request.
        $.ajax({
            url: "/notifications/" + page,
            type: 'get',
            success: function(response){
              var obj = jQuery.parseJSON(response);
              var stored = jQuery.parseJSON(localStorage.getItem(page)) || {};
              var d = new Date();
              var dj = jQuery.parseJSON(JSON.stringify({dt:d}));
              var flag = false;
              $.each(obj, function(key,val) {
                if(!(val.id in stored)){
                  stored[val.id] = {max:val.show_count, time: d,title: val.title, message: val.message, action: val.action,ref:'blank',shown:0};
                }else{
                  stored[val.id] = {max:val.show_count, time: d,title: val.title, message: val.message, action: val.action,ref:'blank',shown:stored[val.id].shown};
                }
                localStorage.setItem(page,JSON.stringify(stored));
                if(stored[val.id].shown < val.show_count){ 
                  flag = true;
                }
              });
              if(flag){
                showSwal(rand,dj.dt,page);
              }            
            }
        });
    }

    function showSwal(rand, d,page){
      var stored = jQuery.parseJSON(localStorage.getItem(page));
      $.each(stored, function(key,notification) {
        if(notification.shown < notification.max && notification.time == d && notification.ref != rand){
          bootbox.alert({
            title:notification.title,
            message:notification.message,
            buttons: {
                ok: {
                    label: 'Ok!',
                    className: 'btn-success'
                },
            },
            callback: function () {
              stored[key].shown++;
              stored[key].ref = rand;
              localStorage.setItem(page,JSON.stringify(stored));
              showSwal(rand, d,page);
            }
          });
        }
      });
    }
    function tourStart(){
        
    }
    function switchTheme(th = ''){
      if(th == ''){
        var now = new Date();
        if(now.getHours() >= 6 && now.getHours() < 18){
          var expires = new Date(now.getFullYear(), now.getMonth(), now.getDate(), 18, 0, 0, 0);
        }else if(now.getHours() >= 18 || now.getHours() < 6){
          var expires = new Date();
          expires.setDate(expires.getDate() + 1);
          expires.setHours( 6, 0, 0, 0);
        }
        console.log(expires);
        expires = "expires=" + expires.toGMTString();
        var newmod = $('html').hasClass('dark') ? 'light' : 'dark';
        document.cookie = "modechanged=" + newmod + ";" + expires + ";path=/";
        $('html').toggleClass('dark');
        $.get('/toggleTheme', function() {});
      }else if(th == 'dark' && !$('html').hasClass('dark')){
        $('html').addClass('dark')
        $.get('/toggleTheme/dark', function() {});
      }else if(th == 'light' && $('html').hasClass('dark')){
        $('html').removeClass('dark')
        $.get('/toggleTheme/light', function() {});
      }
    }
    function showSnow(){
        var snow = 0;//getCookie("snowtime");
        if(snow == 0){
            setCookie("snowtime",1,0.03846);
            $('.snowflakes').removeClass('hide');
            $(".color1").css("color", '#'+Math.random().toString(16).substr(-6));
            $(".color2").css("color", '#'+Math.random().toString(16).substr(-6));
            $(".color3").css("color", '#'+Math.random().toString(16).substr(-6));
            $(".color4").css("color", '#'+Math.random().toString(16).substr(-6));
            $(".color5").css("color", '#'+Math.random().toString(16).substr(-6));
            $(".color6").css("color", '#'+Math.random().toString(16).substr(-6));
            $(".color7").css("color", '#'+Math.random().toString(16).substr(-6));
            $(".color8").css("color", '#'+Math.random().toString(16).substr(-6));
            $(".color9").css("color", '#'+Math.random().toString(16).substr(-6));
            $(".color10").css("color", '#'+Math.random().toString(16).substr(-6));
        }
    }
